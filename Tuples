#Add an item
tuplex=(4,6,2,8,3,1)
print(tuplex)
tuplex=tuplex+(9,)
print(tuplex)
tuplex=tuplex[:5]+(15,20,25)+tuplex[:5]
print(tuplex)
listx=list(tuplex)
listx.append(30)
tuplex=tuple(listx)
print(tuplex)


#Length of a tuple
t=tuple(input())
print(t)
print(len(t))


#Reverse a Tuple
t=input()
print(tuple(reversed(t)))


#Repeated element in a tuple
t=(2,4,5,6,2,3,4,4,7)
print(t)
n=int(input())
print(t.count(n))
